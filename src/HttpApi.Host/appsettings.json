{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=CertManagerDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "AllowedOrigins": [ "http://localhost:4200", "http://localhost:3000" ],
  "AllowedHosts": "*",
  "OpenApiSettings": {
    "EnableSwagger": false,
    "EnableScalar": false
  }
}
